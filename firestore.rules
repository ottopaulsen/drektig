rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow get: if request.auth.token.email == "ottpau@gmail.com";
      allow read: if request.auth.token.email == "ottpau@gmail.com";
      allow list: if request.auth.token.email == "ottpau@gmail.com";
      allow write: if request.auth.token.email == "ottpau@gmail.com";
    }
  }
}